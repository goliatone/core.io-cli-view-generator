{% import './__inputs.swig' as form -%}
<!--
Autogenerated file: {{label}}
{{now|date('Y-m-d h:m:s')}}
-->
<div id="form-content" class="content padded">
    <form id="main-form" method="<%= method %>" action="<%= action %>">
        {%- for prop in fields %}
            {{ prop|json|safe -}}
            {{- form.input(prop.element, prop) -}}
        {%- endfor %}
        {{- form.button()}}
    </form>
</div>
